{
  "name": "Major League Hacking Timezone Automator",
  "version": "1.0",
  "description": "Convert time on https://organize.mlh.io/ to local timezone.",
  "permissions": ["activeTab", "declarativeContent"],
  "options_page": "options.html",
  "content_scripts": [{
    "matches": ["http://organize.mlh.io/participants/events/*", "https://organize.mlh.io/participants/events/*"],
    "js": ["code.js"],
    "run_at": "document_end"
  }],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/time16.png",
      "32": "images/time32.png",
      "48": "images/time48.png",
      "128": "images/time128.png"
    }
  },
  "icons": {
    "16": "images/time16.png",
    "32": "images/time32.png",
    "48": "images/time48.png",
    "128": "images/time128.png"
  },
  "manifest_version": 2
}
